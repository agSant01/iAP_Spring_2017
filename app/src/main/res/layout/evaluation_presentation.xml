<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteY="25dp"
    tools:layout_editor_absoluteX="0dp">

    <android.support.constraint.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.398"
        app:layout_constraintHorizontal_bias="1.0"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp">

        <ImageView
            android:id="@+id/minus_tech_pres"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_marginBottom="0dp"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="0dp"
            android:paddingEnd="18dp"
            app:layout_constraintBottom_toBottomOf="@+id/tx1"
            app:layout_constraintLeft_toLeftOf="@+id/pres_rating_tech"
            app:layout_constraintTop_toTopOf="@+id/tx1"
            app:srcCompat="@drawable/minus_icon" />

        <ImageView
            android:id="@+id/plus_tech_pres"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_marginBottom="0dp"
            android:layout_marginEnd="24dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="0dp"
            android:paddingStart="18dp"
            app:layout_constraintBottom_toBottomOf="@+id/tx1"
            app:layout_constraintLeft_toRightOf="@+id/tx1"
            app:layout_constraintTop_toTopOf="@+id/tx1"
            app:layout_constraintVertical_bias="0.545"
            app:srcCompat="@drawable/plus_icon"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="@+id/pres_rating_tech"
            app:layout_constraintHorizontal_bias="1.0" />

        <ImageView
            android:id="@+id/minus_pres_pres"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="8dp"
            android:paddingEnd="18dp"
            app:layout_constraintBottom_toBottomOf="@+id/tx2"
            app:layout_constraintLeft_toLeftOf="@+id/pres_rating_pres"
            app:layout_constraintTop_toTopOf="@+id/tx2"
            app:srcCompat="@drawable/minus_icon" />

        <ImageView
            android:id="@+id/plus_pres_pres"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_marginBottom="0dp"
            android:layout_marginEnd="24dp"
            android:layout_marginRight="0dp"
            android:layout_marginTop="0dp"
            android:paddingStart="18dp"
            app:layout_constraintBottom_toTopOf="@+id/view2"
            app:layout_constraintRight_toRightOf="@+id/pres_rating_pres"
            app:layout_constraintTop_toTopOf="@+id/tx2"
            app:srcCompat="@drawable/plus_icon" />

        <ImageView
            android:id="@+id/minus_meth_pres"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_marginBottom="0dp"
            android:layout_marginEnd="0dp"
            android:layout_marginLeft="8dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="0dp"
            android:paddingEnd="18dp"
            app:layout_constraintBottom_toTopOf="@+id/view10"
            app:layout_constraintLeft_toLeftOf="@+id/pres_rating_method"
            app:layout_constraintTop_toTopOf="@+id/tx3"
            app:layout_constraintVertical_bias="0.111"
            app:srcCompat="@drawable/minus_icon" />

        <ImageView
            android:id="@+id/plus_meth_pres"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_marginBottom="0dp"
            android:layout_marginEnd="24dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="0dp"
            android:paddingStart="18dp"
            app:layout_constraintBottom_toTopOf="@+id/view10"
            app:layout_constraintRight_toRightOf="@+id/pres_rating_method"
            app:layout_constraintTop_toTopOf="@+id/tx3"
            app:srcCompat="@drawable/plus_icon" />

        <ImageView
            android:id="@+id/minus_res_pres"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_marginBottom="0dp"
            android:layout_marginLeft="8dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="8dp"
            android:paddingEnd="18dp"
            app:layout_constraintBottom_toTopOf="@+id/view9"
            app:layout_constraintLeft_toLeftOf="@+id/pres_rating_res"
            app:layout_constraintTop_toTopOf="@+id/tx4"
            app:srcCompat="@drawable/minus_icon" />

        <ImageView
            android:id="@+id/plus_res_pres"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_marginBottom="0dp"
            android:layout_marginEnd="24dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="0dp"
            android:paddingStart="18dp"
            app:layout_constraintBottom_toTopOf="@+id/view9"
            app:layout_constraintRight_toRightOf="@+id/pres_rating_res"
            app:layout_constraintTop_toTopOf="@+id/tx4"
            app:srcCompat="@drawable/plus_icon" />

        <me.zhanghai.android.materialratingbar.MaterialRatingBar
            android:id="@+id/pres_rating_tech"
            android:layout_width="0dp"
            android:layout_height="50dp"

            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="10dp"
            android:paddingEnd="20dp"
            android:paddingStart="20dp"
            android:stepSize="1"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view1" />

        <TextView
            android:id="@+id/tx2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="18dp"
            android:text="Presentation"
            android:textColor="@color/appGrey"
            android:textSize="20sp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/pres_rating_tech" />

        <me.zhanghai.android.materialratingbar.MaterialRatingBar
            android:id="@+id/pres_rating_method"
            android:layout_width="0dp"
            android:layout_height="50dp"

            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="8dp"
            android:isIndicator="false"
            android:numStars="5"
            android:paddingEnd="20dp"
            android:paddingStart="20dp"
            android:stepSize="1"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/view10" />

        <me.zhanghai.android.materialratingbar.MaterialRatingBar
            android:id="@+id/pres_rating_pres"
            android:layout_width="0dp"
            android:layout_height="50dp"

            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="10dp"
            android:paddingEnd="20dp"
            android:paddingStart="20dp"
            android:stepSize="1"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/view2" />

        <TextView
            android:id="@+id/tx3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="18dp"
            android:text="Methodology"
            android:textColor="@color/appGrey"
            android:textSize="20sp"
            app:layout_constraintHorizontal_bias="0.502"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/pres_rating_pres" />

        <TextView
            android:id="@+id/tx4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="18dp"
            android:text="Results &amp; Discussion"
            android:textColor="@color/appGrey"
            android:textSize="20sp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/pres_rating_method" />

        <View
            android:id="@+id/view1"
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:layout_marginEnd="32dp"
            android:layout_marginStart="32dp"
            android:layout_marginTop="1dp"
            android:background="@android:color/darker_gray"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tx1" />

        <TextView
            android:id="@+id/tx1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="Technical Content"
            android:textColor="@color/appGrey"
            android:textSize="20sp"
            app:layout_constraintHorizontal_bias="0.502"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/view2"
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:layout_marginEnd="32dp"
            android:layout_marginStart="32dp"
            android:layout_marginTop="2dp"
            android:background="@android:color/darker_gray"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tx2" />

        <View
            android:id="@+id/view10"
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:layout_marginEnd="32dp"
            android:layout_marginStart="32dp"
            android:layout_marginTop="2dp"
            android:background="@android:color/darker_gray"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tx3" />

        <me.zhanghai.android.materialratingbar.MaterialRatingBar
            android:id="@+id/pres_rating_res"
            android:layout_width="0dp"
            android:layout_height="50dp"

            android:layout_marginBottom="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="4dp"
            android:paddingEnd="20dp"
            android:paddingStart="20dp"
            android:stepSize="1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/view9"
            app:layout_constraintVertical_bias="0.5" />

        <View
            android:id="@+id/view9"
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:layout_marginEnd="32dp"
            android:layout_marginStart="32dp"
            android:layout_marginTop="2dp"
            android:background="@android:color/darker_gray"
            android:paddingEnd="32dp"
            android:paddingStart="32dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tx4" />
    </android.support.constraint.ConstraintLayout>


</android.support.constraint.ConstraintLayout>